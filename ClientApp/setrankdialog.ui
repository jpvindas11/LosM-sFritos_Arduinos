   <string notr="true">/* ============================================
   DI√ÅLOGO CAMBIAR RANGO - TEMA NE√ìN CYBERPUNK
   ============================================ */

QDialog {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                                stop:0 #0a0e27, stop:0.5 #16213e, stop:1 #0a0e27);
    border: 3px solid #7b2ff7;
    border-radius: 15px;
}

/* ============================================
   LABEL INFO
   ============================================ */

QLabel#label_info {
    color: #00ffff;
    font-size: 18px;
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 rgba(123, 47, 247, 150),
                                stop:0.5 rgba(0, 212, 255, 150),
                                stop:1 rgba(255, 0, 110, 150));
    border: 2px solid #00d4ff;
    border-radius: 12px;
    padding: 12px;
}

/* ============================================
   GROUPBOX - TIPO DE USUARIO
   ============================================ */

QGroupBox {
    background: rgba(26, 35, 62, 150);
    border: 2px solid #7b2ff7;
    border-radius: 12px;
    color: #00ffff;
    font-size: 14px;
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
    padding-top: 25px;
    margin-top: 10px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 5px 15px;
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #7b2ff7, stop:1 #00d4ff);
    border-radius: 8px;
    color: white;
}

/* ============================================
   RADIO BUTTONS
   ============================================ */

QRadioButton {
    color: #ffffff;
    font-size: 13px;
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
    spacing: 8px;
    padding: 5px;
}

QRadioButton::indicator {
    width: 20px;
    height: 20px;
    border: 2px solid #7b2ff7;
    border-radius: 10px;
    background: rgba(26, 35, 62, 180);
}

QRadioButton::indicator:hover {
    border: 2px solid #00d4ff;
    background: rgba(26, 35, 62, 220);
    box-shadow: 0 0 10px rgba(0, 212, 255, 0.5);
}

QRadioButton::indicator:checked {
    background: qradialgradient(cx:0.5, cy:0.5, radius:0.5,
                                fx:0.5, fy:0.5,
                                stop:0 #00ffff, stop:0.5 #00d4ff, stop:1 #7b2ff7);
    border: 2px solid #00ffff;
    box-shadow: 0 0 15px rgba(0, 255, 255, 0.8);
}

QRadioButton:hover {
    color: #00ffff;
}

QRadioButton:checked {
    color: #00ffff;
}

/* Radio Button Due√±o con estilo especial */
QRadioButton#radioButton_o::indicator {
    border: 2px solid #ff006e;
}

QRadioButton#radioButton_o::indicator:checked {
    background: qradialgradient(cx:0.5, cy:0.5, radius:0.5,
                                fx:0.5, fy:0.5,
                                stop:0 #ff006e, stop:0.5 #ff4499, stop:1 #7b2ff7);
    border: 2px solid #ff006e;
    box-shadow: 0 0 15px rgba(255, 0, 110, 0.8);
}

/* ============================================
   DIALOG BUTTON BOX
   ============================================ */

QDialogButtonBox {
    background: transparent;
}

QDialogButtonBox QPushButton[text=&quot;OK&quot;],
QDialogButtonBox QPushButton[text=&quot;Aceptar&quot;] {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #00d4ff, stop:1 #7b2ff7);
    border: 2px solid #00d4ff;
    border-radius: 12px;
    padding: 12px 30px;
    color: white;
    font-size: 14px;
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
    min-width: 120px;
    min-height: 40px;
}

QDialogButtonBox QPushButton[text=&quot;OK&quot;]:hover,
QDialogButtonBox QPushButton[text=&quot;Aceptar&quot;]:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #00e5ff, stop:1 #9351ff);
    box-shadow: 0 0 25px rgba(0, 212, 255, 0.8);
}

QDialogButtonBox QPushButton[text=&quot;OK&quot;]:pressed,
QDialogButtonBox QPushButton[text=&quot;Aceptar&quot;]:pressed {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #00a8cc, stop:1 #6020dd);
    padding-top: 14px;
    padding-bottom: 10px;
}

QDialogButtonBox QPushButton[text=&quot;Cancel&quot;],
QDialogButtonBox QPushButton[text=&quot;Cancelar&quot;] {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #ff006e, stop:1 #ff4444);
    border: 2px solid #ff006e;
    border-radius: 12px;
    padding: 12px 30px;
    color: white;
    font-size: 14px;
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
    min-width: 120px;
    min-height: 40px;
}

QDialogButtonBox QPushButton[text=&quot;Cancel&quot;]:hover,
QDialogButtonBox QPushButton[text=&quot;Cancelar&quot;]:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #ff1a7a, stop:1 #ff6666);
    box-shadow: 0 0 25px rgba(255, 0, 110, 0.8);
}

QDialogButtonBox QPushButton[text=&quot;Cancel&quot;]:pressed,
QDialogButtonBox QPushButton[text=&quot;Cancelar&quot;]:pressed {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #cc0058, stop:1 #cc3333);
    padding-top: 14px;
    padding-bottom: 10px;
}</string>
