    color: white;
    font-size: 24px;
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
    min-width: 140px;
    min-height: 140px;
}

QPushButton#pc1_button:hover,
QPushButton#pc2_button:hover,
QPushButton#pc3_button:hover,
QPushButton#pc4_button:hover {
