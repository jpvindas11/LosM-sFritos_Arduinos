                                stop:0 #9351ff, stop:1 #00e5ff);
    box-shadow: 0 0 20px rgba(123, 47, 247, 0.7);
}

/* ============================================
   BOTONES DE COMPUTADORAS (PC 1-4)
   ============================================ */

QPushButton#pc1_button,
QPushButton#pc2_button,
QPushButton#pc3_button,
QPushButton#pc4_button {
