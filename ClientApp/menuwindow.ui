   <string notr="true">/* ============================================
   ESTILOS GLOBALES - TEMA NE√ìN CYBERPUNK
   ============================================ */

QMainWindow {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                                stop:0 #0a0e27, stop:0.5 #16213e, stop:1 #0a0e27);
}

QWidget#centralwidget {
    background: transparent;
}

/* ============================================
   SIDEBAR - MEN√ö LATERAL
   ============================================ */

QGroupBox {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 rgba(20, 30, 60, 220), 
                                stop:1 rgba(26, 35, 62, 200));
    border: 2px solid #00d4ff;
    border-radius: 10px;
    color: #00ffff;
    font-size: 16px;
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
    padding-top: 20px;
    margin-top: 10px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 5px 15px;
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #ff006e, stop:1 #00d4ff);
    border-radius: 8px;
    color: white;
}

/* ============================================
   BOTONES DEL SIDEBAR
   ============================================ */

QGroupBox QPushButton {
    background: rgba(26, 35, 62, 150);
    border: 2px solid #7b2ff7;
    border-radius: 12px;
    padding: 10px;
    color: #ffffff;
    font-size: 13px;
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
    text-align: left;
    padding-left: 15px;
}

QGroupBox QPushButton:hover {
    background: rgba(123, 47, 247, 100);
    border: 2px solid #9351ff;
    box-shadow: 0 0 15px rgba(123, 47, 247, 0.6);
}

QGroupBox QPushButton:pressed {
    background: rgba(123, 47, 247, 150);
    padding-top: 12px;
    padding-bottom: 8px;
}

QGroupBox QPushButton:disabled {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #ff006e, stop:1 #00d4ff);
    border: 2px solid #00ffff;
    box-shadow: 0 0 20px rgba(0, 255, 255, 0.8);
    color: white;
}

/* ============================================
   BOT√ìN CERRAR SESI√ìN
   ============================================ */

QPushButton#b_cerrarSesion {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #ff006e, stop:1 #ff4444);
    border: 2px solid #ff006e;
    border-radius: 15px;
    padding: 12px;
    color: white;
    font-size: 13px;
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
}

QPushButton#b_cerrarSesion:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #ff1a7a, stop:1 #ff6666);
    box-shadow: 0 0 20px rgba(255, 0, 110, 0.8);
}

/* ============================================
   T√çTULO PRINCIPAL
   ============================================ */

QLabel#main_label {
    color: #00ffff;
    font-size: 28px;
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
    background: transparent;
    padding: 10px;
}

/* ============================================
   BOTONES DE ACCI√ìN (Agregar, Eliminar, etc)
   ============================================ */

QPushButton#user_add,
QPushButton#user_delete,
QPushButton#user_change_pass,
QPushButton#user_change_rank,
QPushButton#arduino_turn {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #7b2ff7, stop:1 #00d4ff);
    border: none;
    border-radius: 15px;
    padding: 10px 20px;
    color: white;
    font-size: 13px;
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
    min-height: 35px;
    min-width: 120px;
}

QPushButton#user_add:hover,
QPushButton#user_delete:hover,
QPushButton#user_change_pass:hover,
QPushButton#user_change_rank:hover,
