                                stop:1 rgba(0, 212, 255, 150));
    border: 3px solid #00ffff;
    box-shadow: 0 0 30px rgba(0, 212, 255, 0.8);
}

QPushButton#pc1_button:pressed,
QPushButton#pc2_button:pressed,
QPushButton#pc3_button:pressed,
QPushButton#pc4_button:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 rgba(123, 47, 247, 200),
                                stop:1 rgba(0, 212, 255, 200));
}

/* ============================================
   LISTAS (QListWidget)
   ============================================ */

QListWidget {
    background-color: rgba(26, 35, 62, 180);
    border: 2px solid #00d4ff;
    border-radius: 15px;
    color: white;
    font-size: 13px;
    font-family: 'Segoe UI', Arial, sans-serif;
    padding: 10px;
}

QListWidget::item {
    padding: 8px;
    border-radius: 8px;
    margin: 2px;
}

QListWidget::item:hover {
    background: rgba(123, 47, 247, 100);
    border: 1px solid #9351ff;
}

QListWidget::item:selected {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #7b2ff7, stop:1 #00d4ff);
    border: 1px solid #00ffff;
    color: white;
}

/* ============================================
   ETIQUETAS
   ============================================ */

QLabel#label_cambios {
    color: #ff006e;
    font-size: 14px;
    font-weight: bold;
    font-family: 'Segoe UI', Arial, sans-serif;
    background: transparent;
}

/* ============================================
   MENU BAR Y STATUS BAR
   ============================================ */

QMenuBar {
    background-color: #0a0e27;
    color: #00d4ff;
    border-bottom: 2px solid #00d4ff;
    padding: 3px;
}

QMenuBar::item:selected {
    background-color: #16213e;
    border-radius: 5px;
}

QStatusBar {
    background-color: #0a0e27;
    color: #00d4ff;
    border-top: 2px solid #00d4ff;
}

/* ============================================
   SCROLLBARS
   ============================================ */

QScrollBar:vertical {
    background: rgba(26, 35, 62, 100);
    width: 12px;
    border-radius: 6px;
}

QScrollBar::handle:vertical {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #7b2ff7, stop:1 #00d4ff);
    border-radius: 6px;
    min-height: 20px;
}

QScrollBar::handle:vertical:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #9351ff, stop:1 #00e5ff);
}

QScrollBar:horizontal {
    background: rgba(26, 35, 62, 100);
    height: 12px;
    border-radius: 6px;
}

QScrollBar::handle:horizontal {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #7b2ff7, stop:1 #00d4ff);
    border-radius: 6px;
    min-width: 20px;
}

QScrollBar::add-line, QScrollBar::sub-line {
    background: none;
    border: none;
}

QScrollBar::add-page, QScrollBar::sub-page {
    background: none;
}</string>
